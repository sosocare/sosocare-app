{
  "expo": {
    "name": "Sosocare",
    "slug": "soso-care-app",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#52A56E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you use for your profile."
        }
      ]
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.app.sosocare",
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sosocare.android",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/appIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "40ef3511-f8a2-4c08-ab0f-e03050f3c836"
      }
    },
    "owner": "sosocaretech"
  }
}
