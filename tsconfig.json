{
  "extends": "expo/tsconfig.base",
  "baseUrl": ".",
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["src/components", "src/custom.d.ts"],
  "exclude": ["node_modules", "metro.config.js"],
  "paths": {
    "@ali/assets/*": ["assets/*"],
    "@ali/components/*": ["src/components/*"]
    // "@ali/colors/*": ["src/theme/colors"]
  }
}
